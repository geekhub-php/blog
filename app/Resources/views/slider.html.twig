{% block slider %}
    <div class="skdslider col-lg-1">
        <ul id="demo1" class="slides">
            <li>
                <img class="img-rounded" src="{{ asset('img/header3.jpg') }}" />
            </li>
            <li><img class="img-rounded" src="{{ asset('img/header4.jpg') }}" />
            </li>
            <li><img class="img-rounded" src="{{ asset('img/header1.jpg') }}" />
            </li>
            <li><img class="img-rounded" src="{{ asset('img/logo.jpg') }}" />
            </li>
            <li><img class="img-rounded" src="{{ asset('img/header2.jpg') }}" /></li>
        </ul>
    </div>
    <script type="text/javascript">
        jQuery(document).ready(function(){
            jQuery('#demo1').skdslider({delay:10000,
                animationSpeed: 2000,
                showNextPrev:false,
                showPlayButton:false,
                autoSlide:true,
                animationType:'fading'})
        });
    </script>

{% endblock slider %}